{
  "name": "magner",
  "description": "Admin panel that can be used for literally any project",
  "version": "0.1.0",
  "files": [
    "dist"
  ],
  "main": "./dist/magner.es.js",
  "module": "./dist/magner.es.js",
  "exports": {
    ".": "./dist/magner.es.js",
    "./style": "./dist/style.css"
  },
  "types": "./dist/src/core/index.d.ts",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lib": "vite build --config vite.lib.ts",
    "start": "vite preview",
    "serve": "vite build && vite preview",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "lint": "eslint --fix src",
    "test:unit": "jest src --silent",
    "test:e2e": "start-server-and-test dev http-get://localhost:3000 test:e2e-run",
    "test:e2e-run": "cypress run --headless",
    "test:e2e-open": "cypress open -b chrome",
    "analyze": "ts-node -r tsconfig-paths/register ./src/server.ts"
  },
  "dependencies": {
    "@editorjs/editorjs": "^2.22.2",
    "@editorjs/header": "^2.6.1",
    "@editorjs/list": "^1.6.2",
    "editorjs-table": "github:VanishMax/editorjs-table",
    "element-plus": "^1.1.0-beta.7",
    "maska": "^1.4.5",
    "vue": "^3.2.6",
    "vue-i18n": "^9.2.0-beta.3",
    "vue-router": "^4.0.6",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "@vitejs/plugin-vue": "^1.6.0",
    "@vue/compiler-sfc": "^3.2.6",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^2.0.0-rc.13",
    "autoprefixer": "^10.3.3",
    "cypress": "^8.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-vue": "^7.17.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.1.0",
    "jest-transform-stub": "^2.0.0",
    "postcss-custom-media": "^8.0.0",
    "postcss-nested": "^5.0.6",
    "start-server-and-test": "^1.13.1",
    "svgo": "^2.5.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.2",
    "vite": "^2.5.1",
    "vite-plugin-dts": "^0.7.0",
    "vue3-jest": "^27.0.0-alpha.2",
    "vuepress": "^2.0.0-beta.25",
    "vuepress-vite": "^2.0.0-beta.25"
  }
